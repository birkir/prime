    <script type="text/javascript">
        var interval, count = 3;
        function startDownload() {
            interval = setInterval(function() {
                count -= 1;
                if (count == 0) {
                    clearInterval(interval);
                    
                    $('#CountdownMessage').html("Click <b>" +
                        '<a id="downloadLink"' +
                        'href="#"' + 
                        'd:postUrl="http://gitcredentialstore.codeplex.com/releases/captureDownload"' +
                        'd:fileid="672532"' +
                        'd:clickoncepath=""' +
                        'd:redirectToAds="false"' +
                        'onclick="return $.release.fn.downloadFile(this);">' +
                        'here</a></b> if your browser does not start the download automatically.');
                        $.release.fn.downloadFile($('a#downloadLink'));
                    
                } else {
                  $('#Countdown').text(count + ' second' + (count != 1 ? 's' : ''));
                }
            }, 1000);
        }
    </script>
